datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
  provider = "prisma-client-js"
}

model Item {
  id        String   @id @default(cuid())
  name      String
  details   String
  position  String
  quantity  Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}